<?xml version="1.0" ?>
<Project>
    <Details>
        <Name>pj_runmode_retry_at_suite_level_at_project_file</Name>
        <Title>Project for testing retry and runmode tag in test files</Title>
        <Engineer>Annalakshmi</Engineer>
        <State>New</State>
        <Date>04/21/2017</Date>
        <Time>11:59:51</Time>
        <default_onError action="next"/>
    </Details>
    <Testsuites>    
        <Testsuite>
            <path>../suites/framework_tests/ts_retry.xml</path>
            <Execute ExecType="Yes"/>
            <onError action="next" value=""/>
            <impact>impact</impact>
        </Testsuite>
        <!-- RUF support at suite level -->
        <Testsuite> 
            <path>../suites/framework_tests/ts_retry_runmode.xml</path>
            <Execute ExecType="Yes"/>
            <onError action="next" value=""/>
            <runmode type="ruf" value="2"/>
            <impact>impact</impact>
        </Testsuite>
	    <!-- RMT support at suite level -->
	    <Testsuite>
			<path>../suites/framework_tests/ts_retry_runmode.xml</path>
			<onError action="next" value=""/>
			<runmode type="rmt" value="2"/>
			<impact>impact</impact>
	    </Testsuite>
	    <!-- RUP support at suite level -->
	    <Testsuite>
			<path>../suites/framework_tests/ts_retry_runmode.xml</path>
			<onError action="next" value=""/>
			<runmode type="rup" value="2"/>
			<impact>impact</impact>
	    </Testsuite>
	    <!-- verification for runmode -->
	    <Testsuite>
			<path>../suites/framework_tests/verify_runmode_at_suite_level.xml</path>
			<onError action="next" value=""/>
			<impact>impact</impact>
	    </Testsuite>
	    <!-- Retry - if condition not met  -->
	    <Testsuite>
			<path>../suites/framework_tests/ts_retry_at_suite_level.xml</path>
			<onError action="next" value=""/>
			<retry type="if" Condition="testsuite_1_result" Condvalue="ptss" count="2" interval="0"/>
			<impact>impact</impact>
	    </Testsuite>
	    <!-- Retry - if condition met -->
	    <Testsuite>
			<path>../suites/framework_tests/ts_retry_cond_tc_level_if_met.xml</path>
			<onError action="next" value=""/>
			<retry type="if" Condition="testsuite_1_result" Condvalue="PASS" count="2" interval="0"/>
			<impact>impact</impact>
	    </Testsuite>
	</Testsuites>
</Project>
    