<?xml version="1.0" ?>

<data>
	<global>
		<command_params>
		</command_params>
		<verifications>
		</verifications>
	</global>
	<!--
	Use for testing cli cmd 'execute=y/n' behavior
	command by title:
	-->
    <testdata execute="yes" title ="t1_exec_y" >
            <command send="ls NonExistTitleDir"
                     end="\$"
                     verify="v1" />
            <v1 found="yes" search="No such file or directory" />
            <command send="ls NonExistTitleDir_2"
                     end="\$"
                     verify="v2" />
            <v2 found="yes" search="No such file or directory" />
    </testdata>
    <testdata execute="no" title ="t1_exec_n" >
            <command send="ls NonExistTitleDir"
                     end="\$"
                     verify="v1" />
            <v1 found="yes" search="No such file or directory" />
            <command send="ls NonExistTitleDir_2"
                     end="\$"
                     verify="v2" />
            <v2 found="yes" search="No such file or directory" />
    </testdata>
	<!--
	command by row:
